{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.yash.ptg.employee_dashboard.Assignment5",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"Northwind.svc": {
				"uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.yash.ptg.employee_dashboard.Assignment5.view.EmployeeDashboard",
			"type": "XML",
			"async": true,
			"id": "EmployeeDashboard"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.yash.ptg.employee_dashboard.Assignment5.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Northwind.svc",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.yash.ptg.employee_dashboard.Assignment5.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteEmployeeDashboard",
					"pattern": "RouteEmployeeDashboard",
					"target": [
						"TargetEmployeeDashboard"
					]
				},
				{
					"name": "RouteEmployeeDetail",
					"pattern": "navToEmployeeDetail/:path:",
					"target": [
						"EmployeeDetail"
					]
				},
				{
					"name": "RouteOrderDetails",
					"pattern": "navToOrderDetail/{path}/{detailPath}",
					"target": [
						"OrderDetails"
					]
				},
				{
					"name": "RouteProductDetails",
					"pattern": "navToProductDetail/{path}/{orderPath}/{detailPath}",
					"target": [
						"ProductDetails"
					]
				}
			],
			"targets": {
				"TargetEmployeeDashboard": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "EmployeeDashboard"
				},
				"EmployeeDetail": {
					"viewType": "XML",
					"viewName": "EmployeeDetail"
				},
				"OrderDetails": {
					"viewType": "XML",
					"viewName": "OrderDetails"
				},
				"ProductDetails": {
					"viewType": "XML",
					"viewName": "ProductDetails"
				}
			}
		}
	}
}